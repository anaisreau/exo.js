let i = 2
let y = "*"
let z = 4

switch (y){
  case ("*"):
    console.log(i*z);
  break;
  case("-"):
    console.log("It's a soustraction");
  break ; 
  case ("/"):
      console.log("It's a division");
  break ;
  default : 
    console.log ("this type of element is invalid")
}


